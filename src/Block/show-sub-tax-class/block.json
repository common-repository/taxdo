{
	"apiVersion": 2,
	"name": "taxdo/show-sub-tax-class",
	"version": "1.0.0",
	"title": "Show Sub tax class",
	"category": "woocommerce",
	"description": "Adds a select field to let the shopper choose sub tax class.",
	"supports": {
		"html": false,
		"align": false,
		"multiple": false,
		"reusable": false
	},
	"parent": [
		"woocommerce/checkout-order-summary-block",
		"woocommerce/cart-items-block"
	],
	"attributes": {
		"lock": {
			"type": "object",
			"default": {
				"remove": true,
				"move": true
			}
		},
		"text": {
			"type": "string",
			"source": "html",
			"selector": ".wp-block-taxdo-show-sub-tax-class",
			"default": ""
		}
	},
	"textdomain": "taxdo",
	"editorStyle": "file:../style.css"
}
